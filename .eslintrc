{
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["prettier", "jsx-a11y"],
  "env": {
    "es6": true,
    "browser": true
  },
  "ignorePatterns": ["src/lib/web3/ethereumjs-tx.min.js"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 1,
    "import/order": [
      "error",
      {
        "newlines-between": "always"
      }
    ]
  }
}
